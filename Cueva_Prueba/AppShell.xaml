<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Cueva_Prueba.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:Cueva_Prueba.Views"
    xmlns:local="clr-namespace:Cueva_Prueba"
    Title="Cueva_Prueba">

    <TabBar>
        <ShellContent Title="Formulario" ContentTemplate="{DataTemplate views:FormularioPage}" />
        <ShellContent Title="Recetas" ContentTemplate="{DataTemplate views:ListaPage}" />
        <ShellContent Title="Logs" ContentTemplate="{DataTemplate views:LogsPage}" />
    </TabBar>

</Shell>
