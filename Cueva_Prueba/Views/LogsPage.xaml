<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Cueva_Prueba.Views.LogsPage"
             Title="LogsPage">
    <VerticalStackLayout Padding="20">
        <ScrollView>
            <Label Text="{Binding ContenidoLogs}" />
        </ScrollView>
        <Button Text="Abrir carpeta de logs" Clicked="AbrirCarpetaLogs_Clicked" />
    </VerticalStackLayout>
</ContentPage>